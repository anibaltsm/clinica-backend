{
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "provider": {
      "type": "string"
    },
    "password": {
      "type": "password",
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },
    "nombre": {
      "type": "string"
    },
    "apellido": {
      "type": "string"
    },
    "telefono": {
      "type": "string"
    }
  }
}